{
  "name": "aci",
  "version": "0.1.0",
  "description": "ArcGIS Command Line Interface - TypeScript CLI tool for interacting with ArcGIS ecosystem",
  "type": "module",
  "private": true,
  "main": "dist/cli.js",
  "bin": {
    "aci": "dist/cli.js"
  },
  "scripts": {
    "build": "bun build src/cli.ts --outdir dist --target node --format esm",
    "dev": "bun run src/cli.ts",
    "start": "node dist/cli.js",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.0.8"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@esri/arcgis-rest-auth": "^3.8.0",
    "@esri/arcgis-rest-feature-service": "^4.1.0",
    "@esri/arcgis-rest-request": "^4.6.0",
    "@inkjs/ui": "^2.0.0",
    "@types/react": "^19.1.8",
    "chalk": "^5.4.1",
    "chalk-table": "^1.0.2",
    "commander": "^14.0.0",
    "ink": "^6.0.1",
    "keytar": "^7.9.0",
    "react": "^19.1.0",
    "read": "^4.1.0",
    "strip-ansi": "^7.1.0",
    "yaml": "^2.8.0"
  }
}
